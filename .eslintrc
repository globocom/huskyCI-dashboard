{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
  },
  "parserOptions": {
      ecmaFeatures: {
          jsx: true,
      },
      sourceType: "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/jsx-filename-extension": [0]
  },
  "plugins": [ "react" ]
}
